<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.udacityforum">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ForumActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".LoginActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ma"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".NewQuestionActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".sign_in_home"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".signUpHome"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>
